/* RZRP alert */
.alert-danger {
  background-image: none;
  background-color: #fc4c49;
  border-radius: 1px;
  border: none;
  color: #fff;
  font-size: 14px;
  font-weight: 300;
  letter-spacing: 1px;
  text-align: center;
}
/*END Bootstrap Overrides*/
/* Leaflet Overrides*/
.leaflet-popup-close-button + .leaflet-popup-content-wrapper .leaflet-popup-content {
  padding-top: 0px;
}
.leaflet-popup-content-wrapper {
  border-radius: 0px;
  padding: 3px;
  text-align: center;
}
.leaflet-popup-content-wrapper .leaflet-popup-content {
  margin: 5px;
  padding: 0px;
}
/*Map Element*/
#mapid {
