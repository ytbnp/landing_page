@import 'https://fonts.googleapis.com/css?family=Open+Sans';
/*Bootstrap Overrides*/
html {
  font-family: 'Merriweather', 'Helvetica Neue', Arial, sans-serif;
  color: #58595b;
}
html,
body {
  height: 100%;
}
body {
  font: inherit;
  color: inherit;
}
h1 {
  font-size: 3.66rem;
}
h2 {
  font-size: 2.6rem;
}
h3,
h4,
h5 {
  font-weight: 400;
  font-size: 1.53rem;
}
h4 {
  color: #808080;
}
h4 span {
  font-weight: 100;
}
p {
  font-size: 1.2rem;
}
.form-control:focus {
  border-color: #F05F40;
  box-shadow: 0 1px 1px rgba(229, 103, 23, 0.075) inset, 0 0 8px rgba(229, 103, 23, 0.6);
  outline: 0 none;
}
/* RZRP BUTTONS */
.btn-rzrp {
  color: white;
  background-color: #F05F40;
  border-color: #F05F40;
  -webkit-transition: all 0.35s;
  -moz-transition: all 0.35s;
  transition: all 0.35s;
}
.btn-rzrp:hover,
.btn-rzrp:focus,
.btn-rzrp.focus,
.btn-rzrp:active,
.btn-rzrp.active,
.open > .dropdown-toggle.btn-rzrp {
  color: white;
  background-color: #ee4b28;
  border-color: #ed431f;
}
.btn-rzrp:active,
.btn-rzrp.active,
.open > .dropdown-toggle.btn-rzrp {
  background-image: none;
}
.btn-rzrp.disabled,
.btn-rzrp[disabled],
fieldset[disabled] .btn-rzrp,
.btn-rzrp.disabled:hover,
.btn-rzrp[disabled]:hover,
fieldset[disabled] .btn-rzrp:hover,
.btn-rzrp.disabled:focus,
.btn-rzrp[disabled]:focus,
fieldset[disabled] .btn-rzrp:focus,
.btn-rzrp.disabled.focus,
.btn-rzrp[disabled].focus,
fieldset[disabled] .btn-rzrp.focus,
.btn-rzrp.disabled:active,
.btn-rzrp[disabled]:active,
fieldset[disabled] .btn-rzrp:active,
.btn-rzrp.disabled.active,
.btn-rzrp[disabled].active,
fieldset[disabled] .btn-rzrp.active {
  background-color: #F05F40;
  border-color: #F05F40;
}
.btn-rzrp .badge {
  color: #F05F40;
  background-color: white;
}
